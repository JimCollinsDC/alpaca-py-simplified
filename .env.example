# Alpaca API Configuration
# Copy this file to .env and fill in your actual API keys
#
# Get your API keys from: https://app.alpaca.markets/paper/dashboard/overview
# For paper trading, use keys from the paper trading dashboard
# For live trading, use keys from the live trading dashboard

# Alpaca API Keys (required)
ALPACA_API_KEY=your_api_key_here
ALPACA_SECRET_KEY=your_secret_key_here

# Optional: Set to False for live trading (default: True for paper trading)
ALPACA_PAPER=True
